{
  "extends": [
    "eslint",
    "plugin:import/errors",
    "plugin:react",
    "plugin:jsx-a11y",
    "prettier",
    "prettier/react",
    "airbnb-base"
  ],
  "rules": {
    "react/prop-types": 0,
    "jsx-a11y/label-has-for": 0,
    "no-console": 1,
    "strict": 0
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}